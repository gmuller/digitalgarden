---
title: "/{{ page.fileSlug }}/"
permalink: "notes/{{ page.fileSlug | slugify }}/"
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <title>{{ page.fileSlug }}</title>
    {%include "components/pageheader.njk"%}
  </head>
  <body class="theme-{{meta.baseTheme}} markdown-preview-view">
    {%include "components/notegrowthhistory.njk"%}

    <div class="content">
      {% if dgHomeLink !== false%}
        {%include "components/navigation.njk"%}
      {% endif %}
      {{ content | link | highlight | safe}}
      <div>
        <hr />
        <div><small>created: {{ date | truncate(10, true, "") }}</small></div>
        <div><small>updated: {{ lastmod | truncate(10, true, "") }}</small></div>
      </div>
    </div>
  </body>
</html>
